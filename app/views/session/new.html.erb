<div class="conatanier">
  <div class="errors">
    <% if @errors %>
      <ul>
        <% @errors.each do |error| %>
          <li><%= error %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
  <div class="user-form">
    <%= form_with url: session_index_path, local: true do |form| %>
      <%= form.label :username %>
      <%= form.text_field :username, id: "user_username" %>
      <%= form.label :password %>
      <%= form.password_field :password, id: "user_password" %>

      <%= form.submit 'Login' %>
    <% end %>
  </div>
</div>
